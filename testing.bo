const N_ITERATIONS = 42 : u64;

const main = fn() -> u64 {
    let i = N_ITERATIONS;
    let counter = 0 : u64;
    while i > 0 {
        counter = counter + N_ITERATIONS;
        i = i - 1;
    };
    counter
};

