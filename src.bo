import "tests/main.bo";

#entry fn() {
    for #const: thing in (42 : u32, 99 : u8, "hello", 14 : i32) {
        if typeof thing is int {
            print("It's an integer! %\n", thing);
        } else {
            print("It's a string! %\n", thing);
        };
    };
};

