library "base.bo";
import "tests/main.bo";

const slice = fn(buffer: []r u8, start: usize, end: usize) -> []r u8 {
    let new = #0;
    new.ptr = buffer.ptr + start;
    new.len = end - start;
    assert(start <= end);
    assert(end <= buffer.len);
    new
};

#entry fn() -> u64 {
    print(slice("Hi :) this is a text", 4, 10));
    print("\n");

    42 - 13
};
