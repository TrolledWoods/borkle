library "intrinsics.bo";

#entry fn() -> u64 {
    let output = bool_to_string(1 > (0 : u8));
    print(output);
    print(" :) \n");
    
    5
};

const bool_to_string = fn(v: bool) -> []r u8  if v "true" else "false";

const print = fn(string: [] u8) {
    i_stdout_write(string);
    i_stdout_flush();
};
